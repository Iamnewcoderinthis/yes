  	end    
})

local Section = PlayerTab:AddSection({
	Name = "Player"

})

local Section = PlayerTab:AddSection({
    Name = "Movement"

})

PlayerTab:AddSlider({
    Name = "Walkspeed",
    Min = 16,
    Max = 160,
    Default = 5,
    Color = Color3.fromRGB(255,255,255),
    Increment = 1,
    ValueName = "Walkspeed",
    Callback = function(Value)
        print(Value)
        game.Workspace.Players.localPlayer.Character.Humanoid.Walkspeed = Value
    end    
})

PlayerTab:AddSlider({
    Name = "Jump Height",
    Min = 16,
    Max = 500,
    Default = 5,
    Color = Color3.fromRGB(255,255,255),
    Increment = 1,
    ValueName = "Height",
    Callback = function(Value)
     game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
    end    
   })
  End()
    End()

